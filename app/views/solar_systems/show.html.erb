<div id="star">
  <div id="star-name"></div>
  <div id="star-id"><%= @id %></div>
  <div id="star-security"></div>
</div>

<div id="star-moons">
  <b>Known Moons</b>
  <% @moons.each do |moon| %>
    <div><%= link_to "#{moon.name}", moon %></div>
  <% end %>
  
  <b>All Moons</b>
  <div id="all-moons">Searching for moons...</div>
</div>
