<div id="star">
  <div id="star-name"></div>
  <div id="star-id"><%= @id %></div>
  <div id="star-security">Security: </div>
</div>

<div id="star-moons">
  <b>Moons</b>
  <div id="star-moon-count">Tracked Moons: <%= @known_moon_count %></div>
  <% @moons.each do |moon| %>
    <div class="moon"><%= link_to "#{moon.name}", moon %></div>
  <% end %>
</div>
